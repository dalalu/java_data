package ex_05;

import javax.print.attribute.SetOfIntegerSyntax;

public class AnonymouseTest1  {

	public static void main(String[] args) {
		Car fooCar = new Car() {
			public void run() {
				System.out.println("달려라!");
			}
			public void stop() {
				System.out.println("멈...멈춰!!!");
			}
		};
		
		foorCar.run();
		foorCar.stop();
		

	}

}
